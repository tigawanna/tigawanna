    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "rakkasjs": "0.6.20",
    "tailwind-scrollbar": "^3.0.0",
    "tailwindcss": "^3.3.1",
    "tailwindcss-animate": "^1.0.5",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.5"
